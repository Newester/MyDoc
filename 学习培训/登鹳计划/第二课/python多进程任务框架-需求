进程池 = [
    下发一个任务并行跑，结果反馈回来 = [
        资源申请
        任务派发
        执行监控
        资源回收
    ]
    资源开销管理 = [
        资源监控
        资源管理策略
    ]
    高性能 = [
        低消耗:降低进程内存消耗
        高并发:降低CPU占用，IO等待并发，多核并行
    ]
    高可用 = [
        稳定可靠完成任务
        业务恢复能力
        业务不可恢复上报
        识别业务本身的错误造成的意外
    ]
    可复用性高 = [
        高度可定制
        支持复杂任务，比如进程间通信，进程并发
        功能独立
    ]
]